    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.3.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "form-data": "^2.3.3",
    "fs-extra": "^7.0.1",
    "http-status-codes": "^1.3.2",
